<mvc:View
        xmlns:mvc="sap.ui.core.mvc"
        xmlns="sap.m"
        controllerName="de.sapui5buch.demo.controller.Home">
    <!-- <Shell> -->
    <App>
        <pages>
            <Page
                    id="home"
                    title="Demo App">
                <content>
                    <IconTabBar selectedKey="partners">
                        <items>
                            <IconTabFilter
                                    key="table"
                                    text="Table"
                                    icon="sap-icon://card">
                                <mvc:XMLView
                                        id="tableView"
                                        viewName="de.sapui5buch.demo.view.App" />
                            </IconTabFilter>

                            <IconTabFilter
                                    key="personal"
                                    text="Personal"
                                    icon="sap-icon://card">
                                <mvc:XMLView
                                        id="personalView"
                                        viewName="de.sapui5buch.demo.view.Personal" />
                            </IconTabFilter>

                            <IconTabFilter
                                    key="layouts"
                                    text="Layouts"
                                    icon="sap-icon://card">
                                <mvc:XMLView
                                        id="layoutsView"
                                        viewName="de.sapui5buch.demo.view.Layouts" />
                            </IconTabFilter>

                            <IconTabFilter
                                    key="partners"
                                    text="Partners"
                                    icon="sap-icon://card">
                                <mvc:XMLView
                                        id="partnersView"
                                        viewName="de.sapui5buch.demo.view.Partners" />
                            </IconTabFilter>

                            <IconTabFilter
                                    key="partnerSplitt"
                                    text="Partners"
                                    icon="sap-icon://card">
                                <mvc:XMLView
                                        id="partnersViewSplitt"
                                        viewName="de.sapui5buch.demo.view.PartnerSplitt" />
                            </IconTabFilter>

                        </items>
                    </IconTabBar>
                </content>
            </Page>
        </pages>
    </App>
    <!-- </Shell> -->
</mvc:View>